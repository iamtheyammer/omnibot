{
  "name": "omnibot",
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@types/promise-timeout": "^1.3.0",
    "@vercel/ncc": "^0.27.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "terser": "^5.6.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "discord.js": "^12.5.1",
    "ffmpeg-static": "^4.2.7",
    "lodash": "^4.17.20",
    "pkg-install": "^1.0.0",
    "promise-timeout": "^1.3.0"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "inspect": "yarn run --inspect ts-node src/index.ts",
    "build": "ncc build src/index.ts && terser dist/index.js --compress --output dist/omnibot.bundle.js",
    "build:dev": "ncc build src/index.ts --source-map"
  }
}
